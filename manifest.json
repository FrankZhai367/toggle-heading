{
  "name": "Toggle Heading",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "Making headings collapsed",
  "permissions": [
    "storage",
    "bookmarks",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "折叠标题",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
